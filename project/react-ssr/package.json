{
    "name": "my_project",
    "version": "0.1.0",
    "private": true,
    "author": {
        "name": "kanghongyan",
        "email": "khongyan379@sina.com"
    },
    "repository": {
        "type": "git",
        "url": "http://gitlab.xxx.com/groups/FFF-team/_test_ssr"
    },
    "group": "FFF-team",
    "type": "react-ssr",
    "keywords": [
        "earth",
        "react",
        "_test_ssr"
    ],
    "dependencies": {
        "classnames": "2.2.5",
        "core-js": "2.5.7",
        "history": "4.7.2",
        "koa": "^2.7.0",
        "lm-bundle-loader": "0.1.1",
        "lm-flexible": "0.1.1",
        "lm-loading": "0.2.1",
        "prop-types": "15.5.10",
        "qs": "^6.6.0",
        "raf": "3.4.0",
        "react": "16.4.0",
        "react-dom": "16.4.0",
        "react-loadable": "5.5.0",
        "react-redux": "5.0.6",
        "react-router-config": "5.0.1",
        "react-router-dom": "5.0.1",
        "react-ssr-with-koa": "1.0.0-alpha.2.1.0",
        "redux": "3.7.2",
        "redux-thunk": "2.2.0",
        "reselect": "3.0.1"
    },
    "scripts": {
        "start": "earth-scripts start",
        "build": "earth-scripts build",
        "update": "earth-scripts update",
        "test": "earth-scripts test --env=jsdom",
        "mock": "earth-scripts mock",
        "ssr:start": "earth-scripts ssr-start --entry=server.js",
        "ssr:deploy": "earth-scripts ssr-deploy --entry=server.js --env=production",
        "ssr:stop": "earth-scripts ssr-stop",
        "arthur-start": "npm run build && npm run ssr:deploy",
        "arthur-stop": "npm run ssr:stop"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}"
        ],
        "setupFiles": [
            "<rootDir>/node_modules/earth-scripts/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.js?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/node_modules/earth-scripts/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/earth-scripts/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "devDependencies": {
        "earth-scripts": "2.0.0-beta.2.1.0"
    },
    "proxy": "http://localhost:3001/"
}
